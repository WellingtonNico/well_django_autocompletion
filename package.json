{
  "name": "well-django-autocompletion",
  "displayName": "Well Django autocompletion",
  "description": "django autocompletion extension",
  "publisher": "WellingtonNico",
  "version": "0.0.1",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "welldjangoautocompletion.activate",
        "title": "Well Django Autocompletion activate"
      },
      {
        "command": "welldjangoautocompletion.update_cache",
        "title": "Well Django Autocompletion update cache"
      }
    ],
    "grammars": [
      {
        "language": "python",
        "scopeName": "html-embedded-on-python",
        "path": "src/syntaxes/html-embedded-on-python.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WellingtonNico/welldjangoautocompletion"
  },
  "keywords": [
    "html",
    "urls",
    "django",
    "autocomplete",
    "templates"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.89.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.3.9"
  }
}
